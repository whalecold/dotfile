{
  "languageserver": {
    "golang": {
      "command": "gopls",
	  "trace.server": "verbose",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/17.0.3/clangd_17.0.3/bin/clangd",
  "snippets.ultisnips.pythonPrompt": true,
  "git.gitlab.hosts": [
    "gitlab.example.com",
    "gitlab.com",
    "code.byted.org"
  ],
  "git.enableGutters": true,
  "java.enabled": true,
  "java.jdt.ls.java.home": "/Library/Java/JavaVirtualMachines/temurin-17.jdk/Contents/Home",
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -Xlog:disable",
  "java.import.maven.enabled": true,
  "java.trace.server": "verbose",
  "java.format.enabled": true
}
